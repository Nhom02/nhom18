<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IServiceTonKho" />
                <binding name="BasicHttpBinding_IServiceSanPham" />
                <binding name="BasicHttpBinding_IServicePhieuNhap" />
                <binding name="BasicHttpBinding_IServiceChiTietPhieuNhap" />
                <binding name="BasicHttpBinding_IServiceHoaDon" />
                <binding name="BasicHttpBinding_IServiceChiTietHoaDon" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServiceGiayit/ServiceTonKho/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceTonKho"
                contract="ServiceReferenceTonKho.IServiceTonKho" name="BasicHttpBinding_IServiceTonKho" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServiceGiayit/ServiceSanPham/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceSanPham"
                contract="ServiceReferenceSanPham.IServiceSanPham" name="BasicHttpBinding_IServiceSanPham" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServiceGiayit/ServicePhieuNhap/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicePhieuNhap"
                contract="ServiceReferencePhieuNhap.IServicePhieuNhap" name="BasicHttpBinding_IServicePhieuNhap" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServiceGiayit/ServiceChiTietPhieuNhap/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceChiTietPhieuNhap"
                contract="ServiceReferenceChiTietPhieuNhap.IServiceChiTietPhieuNhap"
                name="BasicHttpBinding_IServiceChiTietPhieuNhap" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServiceGiayit/ServiceHoaDon/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceHoaDon"
                contract="ServiceReferenceHoaDon.IServiceHoaDon" name="BasicHttpBinding_IServiceHoaDon" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ServiceGiayit/ServiceChiTietHoaDon/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceChiTietHoaDon"
                contract="ServiceReferenceChiTietHoaDon.IServiceChiTietHoaDon"
                name="BasicHttpBinding_IServiceChiTietHoaDon" />
        </client>
    </system.serviceModel>
</configuration>